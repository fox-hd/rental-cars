<% if @car_categories.empty? %>
  <h1>Nenhuma categoria cadastrada</h1>
<% else %>
  <% @car_categories.each do |category| %>
    <%= link_to category.name, category %>
  <% end %>
<% end %><br>
<%= link_to 'Registrar uma nova categoria', new_car_category_path %><br>
<%= link_to 'Voltar', root_path %>



